<%= render 'book', book: @book %>
<%= render 'book', user: @user %>
    
  <div class="col-md-8 offset-md-1">
    <h2>Users</h2>
    <table class="table table-hover yable-inverse">
      <thead>
        <tr>
          <th></th>
          <th>プロフィール画像</th>
          <th>名前</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
　      　<tr>
　      　  <td><%= image_tag @user.get_profile_image(100,100) %></td>
　　        <td><%= @user.name %></td>
　　        <td><%= link_to "Show", user_path(current_user.id) %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>